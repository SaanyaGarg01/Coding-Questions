class Solution {
    public int sumOddLengthSubarrays(int[] arr) {
       int n=arr.length,sum=0;
        for(int i=0;i<n;i++){
             int left=i+1;
             int right=n-i;
             int subarrays=left*right;
             int oddcount=(subarrays+1)/2;
             sum+=arr[i]*oddcount;
           }
            return sum;
       Â }
